
public class TestaLacos {
	public static void main(String[] args) {
		for (int multiplicacao = 1; multiplicacao <= 10; multiplicacao++) {
			for (int contador = 0; contador <= 10; contador++) {
				System.out.print(contador * multiplicacao);
				System.out.print(" ");
			}
			System.out.println();
		}
	}
}
